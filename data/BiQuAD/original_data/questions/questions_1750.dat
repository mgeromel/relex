{"category": "Simple", "rule": "Q_S_0", "question": "Who won the game?", "datalog": "[:find ?awayteam\n    :where [?m \":match/awayteam\" ?awayteam]\n            [?m \":match/hometeam\" ?hometeam]\n            [?m \":match/awayteam_goals\" ?atg]\n            [?m \":match/hometeam_goals\" ?htg]\n            [(< ?htg ?atg)]\n    ]\n[:find ?hometeam\n    :where [?m \":match/awayteam\" ?awayteam]\n            [?m \":match/hometeam\" ?hometeam]\n            [?m \":match/awayteam_goals\" ?atg]\n            [?m \":match/hometeam_goals\" ?htg]\n            [(> ?htg ?atg)]\n    ]", "answer": "'Tottenham Hotspur'", "answertype": "text", "minute": null}
{"category": "Aggregation", "rule": "Q_A_1", "question": "How many minutes did the game run?", "datalog": "[:find (max ?minute)\n    :where [_ \":event/minute\" ?minute]]", "answer": "94", "answertype": "minute", "minute": "94"}
